# Example secrets file - Copy to secrets.yml and encrypt with Ansible Vault
#
# To create encrypted vault file:
#   cp secrets.yml.example secrets.yml
#   ansible-vault encrypt secrets.yml
#
# To edit encrypted vault file:
#   ansible-vault edit secrets.yml
#
# To run playbook with vault:
#   ansible-playbook playbook/00.ilo_iso_esxi.yaml --ask-vault-pass
#   OR
#   ansible-playbook playbook/00.ilo_iso_esxi.yaml --vault-password-file ~/.vault_pass

# ESXi root password
vault_root_password: 'YourSecurePassword'

# BMC/iLO/iDRAC credentials
vault_ilo_user: 'admin'
vault_ilo_pass: 'YourBMCPassword'
